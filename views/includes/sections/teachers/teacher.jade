.col.s12.m6.l4.xl3
  .teacher-img-container
    img.circle.z-depth-3.teacher-img(src="/images/teachers/#{teacher.pictureUrl}", alt='')
  h5.grey-text.text-darken-4.center
    b #{teacher.name}
  p.deep-orange-text.center.text-uppercase #{teacher.role}
  p.grey-text.center #{teacher.description}
  .flex
    each social in teacher.socials
      a.deep-orange-text(href=social.link)
        i.fab.fa-lg.teacher-social-icon(class="fa-#{social.type}")
